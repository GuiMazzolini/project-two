<div class="title-container">
    <h1>Data Analytics</h1>
    <h5>Search Engine</h5>
</div>

{{#each project}}
    <div class="container text-center">
        <div class="row justify-content-evenly">
            <div class="col-12 col-md-5 col-xl-3 m-1">
                <div class="card" style="width: 18rem;">
                    <h5 class="card-title mt-1">{{name}}</h5>
                    <img src="../public/images/teste.jpg" class="card-img-top">
                    <div class="card-body">
                        <p class="card-text">{{description}}</p>
                        <a href="{{url_website}}" target="_blank" class="btn btn-primary mb-1">{{url_website}}</a>
                        <a href="{{url_website}}" target="_blank" class="btn btn-primary">{{url_website}}</a>
                        <p class="card-text mt-1">Created by: </p>
                    </div>
                </div>
            </div>
        </div>
    </div>
{{/each}}
       
